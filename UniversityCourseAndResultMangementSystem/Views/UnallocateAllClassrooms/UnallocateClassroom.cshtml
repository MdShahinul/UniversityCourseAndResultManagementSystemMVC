
@{
    ViewBag.Title = "UnallocateClassroom";
}

<h2>Unallocate All Classrooms</h2>

<fieldset>
    <form method="post" class="button-center">
 
        <div>
            @if (ViewBag.message != null)
            {
                <h4>@ViewBag.message</h4>
            }
        </div>
        <h3 id="a" style="display:none">Are you sure to Unallocate All Classrooms?</h3>
        <button type="submit" id="clickMeButton" onclick="return  confirm('Do you want to UnAllocate Class Rooms')" class="btn btn-default btn-lg">Unallocate Classrooms</button><br /><br />
    </form>
</fieldset>

